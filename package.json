  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^3.10.0",
    "loopback": "^3.19.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.0.0",
    "nodemon": "^1.17.5",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.0.0-rc.3",
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "nsp": "^3.2.1",
    "onsenui": "^2.10.1",
    "vue-onsenui": "^2.6.1",
    "vue-template-compiler": "^2.5.16"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
